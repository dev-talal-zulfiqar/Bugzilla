<%= form_for([@project] )do |f| %>
  <div class="form-group first">
    <%= f.label :title %><br />
    <%= f.text_field :title, :class => 'form-control', autofocus: true, autocomplete: "title" %>
  </div>

  <div class="form-group last">
    <%= f.label :description %><br />
    <%= f.text_field :description, :class => 'form-control', autocomplete: "description" %>
  </div>

    <div class="form-group last">
      <%= f.label :Members %><br />
      <%= f.select :user_id, @user.map{ |type| [type.name.capitalize, type.id] }, { multiple: true, size: 2 }, :class => 'form-control' %>
    </div>
  <%= f.submit "Create Project", :class => 'btn btn-block btn-primary btn-theme-color mb-2' %>
<% end %>
